  <Container className=\"h-100\">\r\n                    <Row className=\"h-100\">\r\n\r\n                        <Col lg={6} className=\"my-auto\">\r\n                            <div class=\"header-content mx-auto\">\r\n                                <h1 class=\"mb-5\">\r\n                                    <img\r\n                                        src=\"img/LogoLarge.png\"\r\n                                    />\r\n                                </h1>\r\n                                <h2 class=\"mb-5\">\r\n                                    An open source audio synthesizer library for SDL2, written in C++\r\n                                </h2>\r\n                                <ButtonGroup>\r\n                                    <Button variant=\"info\" href=\"https://github.com/airloaf/VSynth/releases\">Download</Button>\r\n                                    <Button variant=\"info\" href=\"https://airloaf.github.io/VSynth/html/annotated.html\">Documentation</Button>\r\n                                    <Button variant=\"info\" href=\"https://github.com/airloaf/VSynth/\"><i class=\"fab fa-github\"></i>{\" GitHub\"}</Button>\r\n                                </ButtonGroup>\r\n                            </div>\r\n                        </Col>\r\n                        <Col lg={6} className=\"my-auto\">\r\n                            <h4>\r\n                                MIDI Player made with VSynth! Here are some recorded tracks.\r\n                            </h4>\r\n                            <Iframe\r\n                                width=\"100%\"\r\n                                height=\"500\"\r\n                                scrolling=\"no\"\r\n                                frameborder=\"no\"\r\n                                allow=\"autoplay\"\r\n                                src=\"https://w.soundcloud.com/player/?url=https%3A//api.soundcloud.com/playlists/1189056859%3Fsecret_token%3Ds-TDUxda0JASZ&color=%23ff5500&auto_play=false&hide_related=false&show_comments=true&show_user=true&show_reposts=false&show_teaser=true&visual=true\"\r\n                            >\r\n                            </Iframe>\r\n                        </Col>\r\n                    </Row>\r\n                </Container>\r\n            </header>\r\n        );\r\n    }\r\n\r\n    features() {\r\n        return (\r\n            <React.Fragment>\r\n\r\n            </React.Fragment>\r\n        );\r\n    }\r\n\r\n    dataflow() {\r\n        return (\r\n            <React.Fragment>\r\n                <section className=\"bg-info\">\r\n                    <Container fluid>\r\n                        <div class=\"section-heading text-center\">\r\n                            <h2>How it works</h2>\r\n                            <p>VSynth is separated into 3 modules:</p>\r\n                            <br />\r\n                            <Row>\r\n                                <Col lg={4}>\r\n                                    <div class=\"feature-item\">\r\n                                        <h3>Middleware</h3>\r\n                                        <p>\r\n                                            Optional component which processes sound data before it reaches the speakers.\r\n                                            Useful for many things such as: outputting to a WAV file, filters, transforms, etc.\r\n                                        </p>\r\n                                    </div>\r\n                                </Col>\r\n                                <Col lg={4}>\r\n                                    <div class=\"feature-item\">\r\n                                        <h3>Synthesizer</h3>\r\n                                        <p>\r\n                                            Main building block of VSynth.\r\n                                            Creates a new thread and samples each of\r\n                                            its sound generators at the given sampling rate.\r\n                                            The sum of the samples are then processed\r\n                                            by its middleware prior to reaching\r\n                                            the hardware speakers.\r\n                                        </p>\r\n                                    </div>\r\n                                </Col>\r\n                                <Col lg={4}>\r\n                                    <div class=\"feature-item\">\r\n                                        <h3>Sound Generator</h3>\r\n                                        <p>\r\n                                            Generates sound data. Think of instruments, sequencers, tones, etc.\r\n                                        </p>\r\n                                    </div>\r\n                                </Col>\r\n                            </Row>\r\n                            <hr />\r\n                        </div>\r\n                        <img\r\n                            src=\"img/VSynthDataFlow.svg\"\r\n                        />\r\n                    </Container>\r\n                </section>\r\n            </React.Fragment>\r\n        );\r\n    }\r\n\r\n    examples() {\r\n        return (\r\n            <React.Fragment>\r\n                <section className=\"\">\r\n                    <Container fluid>\r\n                        <CodeExamples />\r\n                    </Container>\r\n                </section>\r\n            </React.Fragment>\r\n        );\r\n    }\r\n\r\n    render() {\r\n        return (\r\n            <React.Fragment>\r\n                {/* <Navbar /> */}\r\n                {this.intro()}\r\n\r\n                <section class=\"features\" id=\"features\">\r\n                    <div class=\"container\">\r\n                        <div class=\"section-heading text-center\">\r\n                            <h2>Features</h2>\r\n                            <p>With VSynth you have access to many of these features and more!</p>\r\n                            <hr />\r\n                        </div>\r\n                        <div class=\"row\">\r\n                            <div class=\"col my-auto\">\r\n                                <div class=\"container-fluid\">\r\n                                    <div class=\"row\">\r\n                                        <div class=\"col-lg-6\">\r\n                                            <div class=\"feature-item\">\r\n                                                <img src={\"img/Waves.png\"} />\r\n                                                <h3>Basic Waveforms</h3>\r\n                                                <p>All your basic waveforms included! (Sine, Triangle, Square, Sawtooth, Pulse and Noise)</p>\r\n                                            </div>\r\n                                        </div>\r\n                                        <div class=\"col-lg-6\">\r\n                                            <div class=\"feature-item\">\r\n                                                <img src={\"img/ADSR.png\"} />\r\n                                                <h3>ADSR Envelopes</h3>\r\n                                                <p>Control waveform amplitudes with ADSR envelopes!</p>\r\n                                            </div>\r\n                                        </div>\r\n                                    </div>\r\n                                    <div class=\"row\">\r\n                                        <div class=\"col-lg-6\">\r\n                                            <div class=\"feature-item\">\r\n                                                <img src={\"img/Polyphony.png\"} />\r\n                                                <h3>Polyphony</h3>\r\n                                                <p>Play multiple notes, sounds, instruments, etc!</p>\r\n                                            </div>\r\n                                        </div>\r\n                                        <div class=\"col-lg-6\">\r\n                                            <div class=\"feature-item\">\r\n                                                <img src={\"img/WAV.png\"} />\r\n                                                <h3>WAV file output</h3>\r\n                                                <p>Save your synthesized audio to .WAV files!</p>\r\n                                            </div>\r\n                                        </div>\r\n                                    </div>\r\n                                </div>\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n                </section>\r\n                <hr />\r\n                {this.dataflow()}\r\n\r\n                <hr />\r\n                {this.examples()}\r\n\r\n            </React.Fragment>\r\n\r\n        );\r\n    }\r\n}","/mnt/c/Users/vikzi/Programming/Projects/VSynth/docs/landing-page/src/components/Navbar.js",["49"],"import React from 'react';\r\nimport Navbar from 'react-bootstrap/Navbar'\r\n\r\nexport default class VSynthNavbar extends React.Component {\r\n    constructor(props) {\r\n        super(props);\r\n    }\r\n\r\n    render() {\r\n        return (\r\n            <Navbar bg=\"dark\" variant=\"dark\" fixed=\"top\">\r\n                <Navbar.Brand href=\"#home\">\r\n                    <img\r\n                        alt=\"\"\r\n                        src=\"img/LogoSmall.png\"\r\n                        width=\"30\"\r\n                        height=\"30\"\r\n                        className=\"d-inline-block align-top\"\r\n                    />{' '}\r\n                    VSynth\r\n                </Navbar.Brand>\r\n            </Navbar>\r\n\r\n        );\r\n    }\r\n}","/mnt/c/Users/vikzi/Programming/Projects/VSynth/docs/landing-page/src/components/CodeViewer.js",[],["50","51"],"/mnt/c/Users/vikzi/Programming/Projects/VSynth/docs/landing-page/src/components/CodeExamples.js",["52","53","54","55"],"import CodeViewer from \"../components/CodeViewer\"\r\nimport React from \"react\"\r\nimport axios from 'axios';\r\n\r\nimport { Button, Col, Row, Container, Carousel } from \"react-bootstrap\"\r\nimport { CodeBlock, dracula } from \"react-code-blocks\";\r\n\r\nconst NUM_EXAMPLES = 4;\r\n\r\nconst RAW_FILE_URLS = [\r\n    \"https://raw.githubusercontent.com/airloaf/VSynth/master/examples/SimpleTone/src/main.cpp\",\r\n    \"https://raw.githubusercontent.com/airloaf/VSynth/master/examples/LowFrequencyOscillator/src/main.cpp\",\r\n    \"https://raw.githubusercontent.com/airloaf/VSynth/master/examples/piano/src/main.cpp\",\r\n    \"https://raw.githubusercontent.com/airloaf/VSynth/master/examples/SequencedBeat/src/main.cpp\"\r\n]\r\n\r\nconst FILE_URLS = [\r\n    \"https://github.com/airloaf/VSynth/tree/master/examples/SimpleTone\",\r\n    \"https://github.com/airloaf/VSynth/tree/master/examples/LowFrequencyOscillator\",\r\n    \"https://github.com/airloaf/VSynth/tree/master/examples/piano\",\r\n    \"https://github.com/airloaf/VSynth/tree/master/examples/SequencedBeat\"\r\n]\r\n\r\nconst TITLES = [\r\n    \"Simple Sine Wave\",\r\n    \"Low frequency Oscillator on a sawtooth wave\",\r\n    \"Piano with an ADSR envelope\",\r\n    \"Sequencer playing Piano\"\r\n]\r\n\r\nconst WAVES = [\r\n    \"audio/RecordTone.wav\",\r\n    \"audio/LFO.wav\",\r\n    \"audio/Piano.wav\",\r\n    \"audio/SequencedBeat.wav\"\r\n]\r\n\r\nconst DESCRIPTIONS = [\r\n    \"Sine wave playing Middle-C\",\r\n    \"Sawtooth with a low frequency oscillator attached! All waveforms, except for noise(), have a 3rd parameter representing the phase shift/offset. You can attach another waveform to this parameter to create low frequency oscillation.\",\r\n    \"Piano controllable by the keyboard keys. Here the C major scale is being played. Using prebuilt patches that came with VSynth, you can get an instrument up and running fast!\",\r\n    \"Sequencer playing a tune in the background. Piano keys are playable as well, but I didn't play them while recording the sequencer beat.\"\r\n]\r\n\r\nexport default class CodeExamples extends React.Component {\r\n\r\n    constructor(props) {\r\n        super(props)\r\n\r\n        this.state = {\r\n            activeExample: 0\r\n        }\r\n\r\n        this.changeExample = this.changeExample.bind(this);\r\n        this.createCarouselItems = this.createCarouselItems.bind(this);\r\n    }\r\n\r\n    componentDidMount() {\r\n\r\n    }\r\n\r\n    changeExample(amount) {\r\n        const nextExample = (this.state.activeExample + amount) % NUM_EXAMPLES;\r\n        this.setState({ activeExample: nextExample });\r\n    }\r\n\r\n    createCarouselItems() {\r\n        var items = []\r\n\r\n        for (var i = 0; i < NUM_EXAMPLES; i++) {\r\n\r\n            items.push(\r\n                <Carousel.Item>\r\n                    <Row>\r\n                        <Col lg={6} style={\r\n                            {\r\n                                overflowY: \"scroll\",\r\n                                maxHeight: \"50vh\"\r\n\r\n                            }\r\n                        }>\r\n                            <CodeViewer\r\n                                fileURL={RAW_FILE_URLS[i]}\r\n                            />\r\n                        </Col>\r\n                        <Col lg={6}>\r\n                            <h2 class=\"mb-5\" align={\"center\"}>{TITLES[i]}</h2>\r\n                            <h3>{DESCRIPTIONS[i]}</h3>\r\n                            <h4><a href={FILE_URLS[i]}><i class=\"fab fa-github\"></i>View on GitHub!</a></h4>\r\n                            <div align={\"center\"}>\r\n                                <audio controls>\r\n                                    <source src={WAVES[i]} type=\"audio/wav\" />\r\n                                </audio>\r\n                            </div>\r\n                        </Col>\r\n                    </Row>\r\n                </Carousel.Item>\r\n            );\r\n        }\r\n        return items;\r\n    }\r\n\r\n    render() {\r\n        return (\r\n            <React.Fragment>\r\n                <h1 class=\"mb-5\">Code Examples</h1>\r\n                <Carousel interval={null} indicators={false} controls={false} align={\"left\"} activeIndex={this.state.activeExample} >\r\n                    {this.createCarouselItems()}\r\n                </Carousel>\r\n                <Row>\r\n                    <Col lg={2}>\r\n                        <Button onClick={() => { this.changeExample(NUM_EXAMPLES - 1) }} block variant=\"outline-success\">Previous</Button>\r\n                    </Col>\r\n                    <Col lg={8}>\r\n                    </Col>\r\n                    <Col lg={2}>\r\n                        <Button onClick={() => { this.changeExample(1) }} block variant=\"outline-success\">Next</Button>\r\n                    </Col>\r\n                </Row>\r\n            </React.Fragment>\r\n        );\r\n    }\r\n}",{"ruleId":"56","replacedBy":"57"},{"ruleId":"58","replacedBy":"59"},{"ruleId":"60","severity":1,"message":"61","line":1,"column":8,"nodeType":"62","messageId":"63","endLine":1,"endColumn":12},{"ruleId":"60","severity":1,"message":"64","line":3,"column":8,"nodeType":"62","messageId":"63","endLine":3,"endColumn":14},{"ruleId":"60","severity":1,"message":"65","line":7,"column":10,"nodeType":"62","messageId":"63","endLine":7,"endColumn":19},{"ruleId":"60","severity":1,"message":"66","line":7,"column":21,"nodeType":"62","messageId":"63","endLine":7,"endColumn":28},{"ruleId":"60","severity":1,"message":"67","line":8,"column":52,"nodeType":"62","messageId":"63","endLine":8,"endColumn":60},{"ruleId":"68","severity":1,"message":"69","line":43,"column":37,"nodeType":"70","endLine":45,"endColumn":39},{"ruleId":"68","severity":1,"message":"69","line":128,"column":25,"nodeType":"70","endLine":130,"endColumn":27},{"ruleId":"68","severity":1,"message":"69","line":168,